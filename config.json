{
    "formatVersion": 1,
    "menu": {
      "Config.credentialsTab": {
        "label": "Credentials",
        "formFields": {
          "apiEndpoint": {
            "type": "inputText",
            "required": true,
            "label": "API Endpoint",
            "options": {
              "defaultValue": "https://api.emailsys.net/v1/subscribers"
            }
          },
          "username": {
            "type": "inputText",
            "required": true,
            "label": "Username",
            "options": {
              "defaultValue": ""
            }
          },
          "password": {
            "type": "inputText",
            "required": true,
            "label": "Password",
            "options": {
              "defaultValue": "",
              "isPassword": true
            }
          },
          "transferButton": {
            "type": "button",
            "label": "Transfer Newsletter Recipients",
            "options": {
              "buttonLabel": "Transfer",
              "url": "/newsletter-transfer/manual"
            }
          }
        }
      }
    }
  }
  